def secrets():
    return {'bootstrap.servers' : '', # CONFLUENT'S BOOTSTRAP SERVER
            'sasl.username' : '', # CONFLUENT'S SASL USER
            'sasl.password' : '', # CONFLUENT'S SASL PASSWORD
            'url' : '', # CONFLUENT'S SCHEMA REGISTRY URL
            'basic.auth.user.info' : '', # CONFLUNET'S SCHEMA REGISTRY API KEY + SECRET
            }
